{
  "name": "netfiles",
  "version": "0.0.0",
  "private": true,
  "author": "Ronald Dennison <ronniedz@gmail.com>",
  "license": "ISC",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel ./src --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files && cp -r src/views dist/ && cp -r src/public dist/",
    "prestart-dev": "npm run build",
    "start-dev": "nodemon --delay 2 --watch src -e js dist/app.js",
    "start": "nodemon --delay 1 --watch src -e js dist/app.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.17.4"
  },
  "dependencies": {
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.18.0",
    "morgan": "^1.9.0",
    "multiparty": "^4.1.3",
    "pug": "^2.0.3",
    "request": "^2.85.0",
    "rimraf": "^2.6.2"
  }
}
