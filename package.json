{
  "name": "netfiles",
  "version": "0.0.0",
  "private": true,
  "author": "Ronald Dennison <ronniedz@gmail.com>",
  "license": "ISC",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./src --out-dir dist/ --ignore ./node_modules,./.babelrc,./npm-debug.log --copy-files ",
    "prestart-dev": "npm run build",
    "start-dev": "nodemon -w src -e 'json js' -x \"npm run build && node\" dist/app.js",
    "start": "nodemon --delay 1 -w src -e 'json js' dist/app.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.17.4"
  },
  "dependencies": {
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.18.0",
    "morgan": "^1.9.0",
    "multiparty": "^4.1.3",
    "pug": "^2.0.3",
    "request": "^2.85.0",
    "rimraf": "^2.6.2"
  }
}

